{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Schedule Item</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Scheduledintime</th>
                <td>{{ entity.scheduledInTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Scheduledouttime</th>
                <td>{{ entity.scheduledOutTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Clockintime</th>
                <td>{{ entity.clockInTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Clockouttime</th>
                <td>{{ entity.clockOutTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Ispickupinfocomplete</th>
                <td>{{ entity.isPickupInfoComplete }}</td>
            </tr>
            <tr>
                <th>Isdropbackinfocomplete</th>
                <td>{{ entity.isDropbackInfoComplete }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('scheduleitem') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('scheduleitem_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
